import asyncio
from backend.submit_order import run_bot_with_params

BOTS = [
    ("iron_condor", "SPY", 1, 1),
    ("spread", "SPY", 1, 1),
    ("csp", "SPY", 1, 1),
    ("wheel", "SPY", 1, 1),
    ("trend", "SPY", 1, 1),
    ("equity_buy", "QQQ", 1, 0),  # for wealth module
]

async def main() -> None:
    for bot in BOTS:
        bot_name, ticker, contracts, dte = bot
        try:
            print(f"\n--- Running {bot_name} ---")
            result = await run_bot_with_params(bot_name, ticker, contracts, dte)
            print("Success:", result)
        except Exception as e:
            print(f"Failed: {e}")

if __name__ == "__main__":
    asyncio.run(main())
